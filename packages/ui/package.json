{
	"name": "@svonic/ui",
	"version": "1.0.0-next.35",
	"description": "Testing",
	"keywords": [
		"svelte",
		"sveltekit",
		"ionic"
	],
	"homepage": "https://github.com/svonic/test#readme",
	"bugs": {
		"url": "https://github.com/svonic/test/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/svonic/test.git",
		"directory": "packages/ui"
	},
	"license": "MIT",
	"author": "Robert",
	"type": "module",
	"svelte": "index.js",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"package": "svelte-package",
		"preinstall": "npx only-allow pnpm",
		"preview": "vite preview",
		"publish": "npm publish ./package"
	},
	"dependencies": {
		"@mdi/js": "7.0.96",
		"ionicons": "6.0.4",
		"svelte": "3.53.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "1.0.0-next.6",
		"@svonic/core": "workspace:*",
		"@typescript-eslint/eslint-plugin": "5.44.0",
		"@typescript-eslint/parser": "5.44.0",
		"autoprefixer": "10.4.13",
		"eslint": "8.28.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"postcss": "8.4.19",
		"postcss-load-config": "4.0.1",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "2.8.1",
		"svelte-check": "2.9.2",
		"svelte-preprocess": "4.10.7",
		"svelte2tsx": "0.5.20",
		"tailwindcss": "3.2.4",
		"tslib": "2.4.1",
		"typescript": "4.9.3",
		"vite": "3.2.4"
	}
}
