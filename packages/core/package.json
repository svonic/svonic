{
	"name": "@svonic/core",
	"version": "1.0.0-next.35",
	"description": "Testing",
	"keywords": [
		"svelte",
		"sveltekit",
		"ionic"
	],
	"homepage": "https://github.com/svonic/test#readme",
	"bugs": {
		"url": "https://github.com/svonic/test/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/svonic/test.git",
		"directory": "packages/core"
	},
	"license": "MIT",
	"author": "Robert",
	"type": "module",
	"svelte": "index.js",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"package": "svelte-package",
		"preinstall": "npx only-allow pnpm",
		"preview": "vite preview",
		"publish": "npm publish ./package"
	},
	"dependencies": {
		"@ionic/core": "6.4.0",
		"ionicons": "6.0.4",
		"svelte": "3.55.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "1.0.0-next.6",
		"@typescript-eslint/eslint-plugin": "5.46.1",
		"@typescript-eslint/parser": "5.46.1",
		"eslint": "8.29.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"prettier": "2.8.1",
		"prettier-plugin-svelte": "2.9.0",
		"svelte-check": "2.10.2",
		"svelte-preprocess": "5.0.0",
		"svelte2tsx": "0.5.22",
		"tslib": "2.4.1",
		"typescript": "4.9.4",
		"vite": "4.0.1"
	}
}
